<!-- include admin header -->
<%- include('include/_adminHeader')%>
    <!-- include admin header -->

    <!-- Main Site -->

    <!--VIEW STUDENT APPLICATIONS-->
    <h2>Welcome, Placement Cell</h2>
    <h5 class="table-title text-center"><b>View Student Applications</b></h5>
    <div class="table-responsive">
        <table class="table">
            <thead class="thead" style="background-color: #3598fe">
                <tr>
                    <th scope="col">STUDENT ID</th>
                    <th scope="col">FIRST NAME</th>
                    <th scope="col">LAST NAME</th>
                    <th scope="col">COURSE</th>
                    <th scope="col">EMAIL</th>
                    <th scope="col">APPLIED IN</th>
                    <th scope="col">APPLIED FOR</th>
                    <!-- <th scope="col">ACTION</th> -->
                </tr>
            </thead>
            <tbody>
                <%for(var i=0; i < apply.length; i++){%>
                    <tr>
                        <td scope="row" class="font-weight-bold">
                            <%=apply[i].applicant.sId%>
                        </td>
                        <td>
                            <%=apply[i].applicant.firstName%>
                        </td>
                        <td>
                            <%=apply[i].applicant.lastName%>
                        </td>
                        <td>
                            <%=apply[i].applicant.course%>
                        </td>
                        <td>
                            <%=apply[i].applicant.email%>
                        </td>
                        <td>
                            <%=apply[i].companies.companyName%>
                        </td>
                        <td>
                            <%=apply[i].companies.role%>
                        </td>
                    </tr>
                    <%}%>
            </tbody>
        </table>
    </div>

    <!--/VIEW STUDENT APPLICATIONS-->
    <div class="line"></div>
    </div>
    </div>

    <!-- /Main Site -->

    <!-- include admin footer -->
    <%- include('include/_adminFooter')%>
        <!-- include admin footer -->